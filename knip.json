{
	"$schema": "https://unpkg.com/knip@5/schema-jsonc.json",
	"workspaces": {
		"frontend": {
			"project": ["src/**/*.{ts,vue}"],
			"ignore": ["src/env.d.ts", "**/*.test.ts", "**/*.spec.ts"]
		},
		"backend": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.ts"],
			"ignoreBinaries": ["dist/index.js"]
		}
	}
}
